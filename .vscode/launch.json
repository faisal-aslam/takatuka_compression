{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug compress-debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/compress-debug",
            "args": ["tests/test.bin"], 
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,          
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "make"
        }
    ]
}
